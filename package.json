{
  "name": "fab-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/fab-components/fab-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/fab-components/fab-components.esm.js",
      "require": "./dist/fab-components/fab-components.cjs.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "generate:palettes": "node scripts/generate-palettes.js",
    "gen:palettes": "node scripts/gen-palettes.js",
    "copy:config": "node -e \"require('fs').copyFileSync('scripts/palettes.config.json', 'www/palettes.config.json')\"",
    "prebuild": "npm run generate:palettes && npm run gen:palettes && npm run copy:config"
  },
  "devDependencies": {
    "@stencil/core": "^4.38.0",
    "@stencil/store": "^2.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0"
  },
  "license": "MIT",
  "dependencies": {
    "chroma-js": "^3.1.2"
  }
}
